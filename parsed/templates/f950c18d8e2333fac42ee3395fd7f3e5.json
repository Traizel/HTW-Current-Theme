{"components/products/description":"<div class=\"productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}>\n    {{{product.description}}}\n</div>\n\n{{#if product.warranty}}\n    <p class=\"productView-title\">{{lang 'products.warranty'}}</p>\n    <div class=\"productView-description\">\n        {{{product.warranty}}}\n    </div>\n{{/if}}\n","components/products/tabs":"<div class=\"tabs-contents\" data-tab role=\"tablist\">\r\n\t<button type=\"button\" class=\"collapsible is-active\" data-tab-description href=\"#tab-description\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-description\">{{lang 'products.description'}}</button>\r\n\t<div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent is-active\" id=\"tab-description\">\r\n\t\t{{> components/products/description}}\r\n\t\t<br/>\r\n\t</div>\r\n\t{{#if product.videos.list.length}}\r\n\t\t<button type=\"button\" class=\"collapsible\" data-tab-videos href=\"#tab-videos\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-videos\">Videos</button>\r\n\t\t<div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent\" id=\"tab-videos\">\r\n\t\t\t<div class=\"videoGallery-list\">\r\n\t\t\t\t{{#each (limit product.videos.list 4)}}\r\n\t\t\t\t\t<div class=\"videoGallery-item\">\r\n\t\t\t\t\t\t<a href=\"//www.youtube.com/embed/{{id}}\" data-featherlight=\"iframe\" data-featherlight-iframe-allowfullscreen=\"true\">\r\n\t\t\t\t\t\t\t<div class=\"video-figure\">\r\n\t\t\t\t\t\t\t\t<img alt=\"{{title_long}}\" class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/hqdefault.jpg\"/>\r\n\t\t\t\t\t\t\t\t<div class=\"shine\"></div>\r\n\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t</a>\r\n\t\t\t\t\t\t<h4 class=\"videoGallery-title\"><a href=\"//www.youtube.com/embed/{{id}}\" data-featherlight=\"iframe\" data-featherlight-iframe-allowfullscreen=\"true\">{{title_long}}</a></h4>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t{{/each}}\r\n\t\t\t</div>\r\n\t\t\t<link href=\"//cdn.rawgit.com/noelboss/featherlight/1.7.13/release/featherlight.min.css\" type=\"text/css\" rel=\"stylesheet\" />\r\n\t\t\t<script src=\"//code.jquery.com/jquery-latest.min.js\" defer></script>\r\n\t\t\t<script src=\"//cdn.rawgit.com/noelboss/featherlight/1.7.13/release/featherlight.min.js\" type=\"text/javascript\" charset=\"utf-8\" defer></script>\r\n\t\t\t<br/>\r\n\t\t</div>\r\n\t{{/if}}\r\n\t<button style=\"display:none;\" type=\"button\" class=\"collapsible\" data-tab-application-instructions href=\"#tab-application-instructions\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-application-instructions\">Application Instructions</button>\r\n\t<div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent\" id=\"tab-application-instructions\"></div>\r\n\t{{#if product.custom_fields}}\r\n\t\t<button style=\"display:none;\" type=\"button\" class=\"collapsible\" data-tab-shipping href=\"#tab-shipping\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-shipping\">Shipping</button>\r\n\t\t<div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent\" id=\"tab-shipping\">\r\n\t\t\t<ul class=\"shipping-restrictions\">\r\n\t\t\t\t{{#each product.custom_fields}}\r\n\t\t\t\t\t{{#if name '===' 'location'}}\r\n\t\t\t\t\t\t<li class=\"{{name}}\"><strong>{{uppercase value}}</strong> PRODUCT; will make your entire order ship from our {{value}} location.</li>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if name '===' 'location_colors'}}\r\n\t\t\t\t\t\t{{#if value '===' 'all'}}\r\n\t\t\t\t\t\t{{else}}\r\n\t\t\t\t\t\t\t<li class=\"{{name}}\">location specific colors: <strong>{{value}}</strong>.</li>\r\n\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if name '===' 'dropship_colors'}}\r\n\t\t\t\t\t\t<li class=\"{{name}}\">This product has options that are <strong>DROPSHIP ONLY</strong> and, if selected, this material will be sent via ground shipping from the manufacturer and will ship separately from your order.</li>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if name '===' 'shipping'}}\r\n\t\t\t\t\t\t{{#if value '===' 'ground only'}}\r\n\t\t\t\t\t\t\t<li class=\"{{name}}\"><strong>GROUND ONLY</strong> shipping; will make your entire order ship 2-5 day ground.</li>\r\n\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t{{/each}}\r\n\t\t\t</ul>\r\n\t\t</div>\r\n\t\t<button style=\"display:none;\" type=\"button\" class=\"collapsible\" data-tab-blog href=\"#tab-blog\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-blog\">Blog</button>\r\n\t\t<div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent\" id=\"tab-blog\">\r\n\t\t\t<div class=\"productBlogPost-list\"></div>\r\n\t\t\t<br/>\r\n\t\t</div>\r\n\t\t\t<!-- FAQ -->\r\n\t\t<button style=\"display:none;\" type=\"button\" class=\"collapsible\" data-tab-faq href=\"#tab-faq\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-faq\">FAQ</button>\r\n\t\t<div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent\" id=\"tab-faq\">\r\n\t\t\t<ul class=\"faq-question-list\">\r\n\t\t\t\t{{#each product.custom_fields}}\r\n\t\t\t\t\t{{#if name '===' 'faq question'}}\r\n\t\t\t\t\t\t<li class=\"{{dashcase name}}\"><strong>Q: {{value}}</strong></li>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t{{#if name '===' 'faq answer'}}\r\n\t\t\t\t\t\t<li class=\"{{dashcase name}}\"><strong>A:</strong> {{value}}</li>\r\n\t\t\t\t\t{{/if}}\r\n\t\t\t\t{{/each}}\r\n\t\t\t</ul>\r\n\t\t\t<script>\r\n\t\t\t\twindow.addEventListener('DOMContentLoaded', function() {\r\n\t\t\t\t\tif (document.querySelectorAll('.faq-question-list li').length > 1) {\r\n\t\t\t\t\t\tdocument.querySelector('[data-tab-faq]').style.display = '';\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t</script>\r\n\t\t\t<br/>\r\n\t\t</div>\r\n\t\t<!-- /FAQ -->\r\n\t{{/if}}\r\n\t<button style=\"display:none;\" type=\"button\" class=\"collapsible\" data-tab-pricing href=\"#tab-pricing\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" controls=\"tab-pricing\">Pricing</button>\r\n\t<div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content has-jsContent\" id=\"tab-pricing\"></div>\r\n</div>\r\n"}
