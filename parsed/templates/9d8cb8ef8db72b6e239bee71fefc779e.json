{"components/products/videos":"<section class=\"toggle videoGallery\" data-video-gallery>\n    <h4 class=\"toggle-title\">\n        {{lang 'products.videos.header'}}\n        <a href=\"#videoGallery-content\" class=\"toggleLink is-open\" data-collapsible>\n            <span class=\"toggleLink-text toggleLink-text--on\">\n                {{lang 'products.videos.hide'}}\n            </span>\n            <span class=\"toggleLink-text toggleLink-text--off\">\n                {{lang 'products.videos.show'}}\n            </span>\n        </a>\n    </h4>\n\n    <div class=\"toggle-content is-open\" id=\"videoGallery-content\">\n        \n        <ul class=\"videoGallery-list\">\n        {{#each this.list}}\n            <li class=\"videoGallery-item\">\n                <a href=\"//www.youtube.com/embed/{{id}}\" class=\"video {{#if @first}}is-active{{/if}}\" data-featherlight=\"iframe\" data-featherlight-iframe-allowfullscreen=\"true\">\n                    <div class=\"video-figure\">\n                        <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\n                    </div>\n                    <div class=\"video-body\">\n                        <h5 class=\"video-title\">{{title_long}}</h5>\n                        <p class=\"video-description\">{{description_short}}</p>\n                    </div>\n                </a>\n            </li>\n        {{/each}}\n        </ul>\n    </div>\n\n</section>\n"}
