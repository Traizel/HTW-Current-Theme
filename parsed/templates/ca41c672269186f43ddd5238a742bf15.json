{"components/category/education/searchbar":"<section><div>\n    <input type=\"text\" id=\"searchblogs\" name=\"search\" placeholder=\"Search Education\">\n    <input type=\"submit\" id=\"searchblogsbutton\" value=\"Search\" onclick=searchblogs()>\n</div>\n<script>\n   let searchBlogs = document.getElementById('searchblogs')\n    function searchblogs() {\n            searchBlogs = searchBlogs.value\n        window.open(`https://www.heattransferwarehouse.com/search.php?section=content&search_query=${searchBlogs}`, \"_self\");\n    }\n    \n    searchBlogs.addEventListener(\"keyup\", function(event) {\n    if (event.keyCode === 13) {\n        event.preventDefault();\n        document.getElementById(\"searchblogsbutton\").click();\n    }\n});\n</script>\n</section>\n"}
